<!DOCTYPE html>
<html>
  <head>
    <title>DeviseMailer</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_importmap_tags %>
  </head>

  <body>
    <%= notice if notice%>
    <%= alert if alert%>
    <%= yield %>
  </body>
</html>